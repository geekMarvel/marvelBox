
@{
    ViewData["Title"] = "Check";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="bg-primary">Check</h1>
<br/>

<style>
    
    body {
        background: url(http://wrbbradio.org/wp-content/uploads/2016/10/grey-background-07.jpg) no-repeat center fixed;
        background-size: cover;
    }

    .container {
        background: #2d3e3f;
        width: 1750px;
        height: 440px;
        padding-bottom: 20px;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        margin: auto;
        padding: 70px 50px 20px 50px;
    }


    .fl {
        float: left;
        width: 110px;
        line-height: 35px;
    }

    .fontLabel {
        color: white;
    }

    .fr {
        float: right;
    }

    .clr {
        clear: both;
    }

    .box {
        width: 360px;
        height: 35px;
        margin-top: 10px;
        font-family: verdana;
        font-size: 12px;
    }

    .textBox {
        height: 35px;
        width: 190px;
        border: none;
        padding-left: 20px;
    }

    .new {
        float: left;
    }

    .iconBox {
        height: 35px;
        width: 40px;
        line-height: 38px;
        text-align: center;
        background: #ff6600;
    }

    .radio {
        color: white;
        background: #2d3e3f;
        line-height: 38px;
    }

    .terms {
        line-height: 35px;
        text-align: center;
        background: #2d3e3f;
        color: white;
    }

    .submit {
        float: right;
        border: none;
        color: white;
        width: 110px;
        height: 35px;
        background: #ff6600;
        text-transform: uppercase;
        cursor: pointer;
    }

    ::-webkit-input-placeholder { /* Chrome/Opera/Safari */
    }

</style>

<html>
<head>
    <meta charset="utf-8">
    @* <title>Form</title> *@
    <link href="css/style.css" type="text/css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>

   
    <h3 class="accordion-item">free text: @ViewBag.step1</h3>
    <h3 class="accordion-item">is authenticated? @ViewBag.step2</h3>



    <h3 class="accordion-item">free text: @ViewBag.V</h3>
    <h3 class="accordion-item">user name: @ViewBag.D</h3>
    <h3 class="accordion-item">is authenticated? @ViewBag.C</h3>
    <h3 class="accordion-item">authentication type: @ViewBag.Q</h3>

</body>
</html>

